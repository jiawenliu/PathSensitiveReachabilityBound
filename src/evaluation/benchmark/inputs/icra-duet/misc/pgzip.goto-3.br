[ first_pass <- 0 ] 0;
[ quit_flag <- 0 ] 1;
[ i <- 0 ] 2;
while [ < (i, gc_number_of_threads) ] 3 do {
	if [ > (gc_bytes_to_read, 0u1) ] 4
	then {  
	   if [ != (deflate_tc, NULL) ] 5
	   then { [ i <- gc_number_of_threads ] 6 }
	   else { [ skip ] 7 }
	   
	} 
	else { [ i <- gc_number_of_threads ] 8 };
	[ i <- + (i, 1) ] 9
}
























